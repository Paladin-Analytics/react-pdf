{
  "name": "@surge-global-engineering/rpdf-pdfkit",
  "version": "2.0.20",
  "description": "A PDF generation library for Node.js",
  "main": "lib/pdfkit.cjs.js",
  "module": "lib/pdfkit.es.js",
  "repository": "git@github.com:SURGE-GLOBAL-ENGINEERING/react-pdf.git",
  "browser": {
    "./lib/pdfkit.es.js": "./lib/pdfkit.browser.es.js",
    "./lib/pdfkit.cjs.js": "./lib/pdfkit.browser.cjs.js"
  },
  "author": {
    "name": "Devon Govett",
    "email": "devongovett@gmail.com",
    "url": "http://badassjs.com/"
  },
  "scripts": {
    "clear": "rimraf ./lib ./src/font/data/*.json",
    "parse:afm": "babel-node ./src/font/data/compressData.js",
    "build": "npm run clear && npm run parse:afm && rollup -c ",
    "watch": "npm run clear && npm run parse:afm && rollup -c -w"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "@surge-global-engineering/rpdf-fontkit": "^2.0.17",
    "@surge-global-engineering/rpdf-png-js": "^2.0.11",
    "crypto-js": "^4.0.0"
  },
  "devDependencies": {
    "blob-stream": "^0.1.2",
    "iconv-lite": "^0.4.13"
  },
  "gitHead": "01919e821e47dff502395f6000f1db85ab0d94e7"
}
